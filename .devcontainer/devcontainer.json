{
  "name": "A fully opensource flow for SCL180 PDKs",

  "build": {
    "dockerfile": "Dockerfile",
    "context": ".."
  },

  "forwardPorts": [6080],

  "portsAttributes": {
    "6080": {
      "label": "noVNC Desktop",
      "onAutoForward": "openBrowser"
    }
  },

  "overrideCommand": false,
  "remoteUser": "vscode",

  "containerEnv": {
    "DISPLAY": ":1"
  },
"postCreateCommand": "/bin/bash .devcontainer/install-openroad.sh"
}




